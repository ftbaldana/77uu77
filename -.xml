  <script src="https://cdn.jsdelivr.net/gh/ftbaldana/77uu77@main/TemplateData/stlyes.js"></script>
  <script src="https://cdn.jsdelivr.net/gh/ftbaldana/77uu77@main/Build/UnityLoader.js"></script>
  <script src="https://cdn.jsdelivr.net/gh/ftbaldana/77uu77@main/Manager.js"></script>
